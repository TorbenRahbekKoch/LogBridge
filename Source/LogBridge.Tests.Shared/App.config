<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
    <section name="logBridge" type="SoftwarePassion.LogBridge.LogBridgeConfigurationSection, LogBridge"/>
  </configSections>

  <logBridge logWrapperType="SoftwarePassion.LogBridge.Tests.Shared.LogWrapperStrategy" logWrapperAssembly="SoftwarePassion.LogBridge.Tests.Shared, Version=1.3.1.0, Culture=neutral, PublicKeyToken=null" throwOnResolverFail="true" internalDiagnosticsEnabled="true">
    <extendedProperties>
      <add name="Property1" value="Value1"/>
      <add name="Property2" value="Value2"/>
    </extendedProperties>
  </logBridge>

  <appSettings>
  </appSettings>

  <log4net>
    <appender name="MemoryAppender" type="log4net.Appender.MemoryAppender"/>

    <root>
      <level value="ALL"/>
      <!--<appender-ref ref="LogFileAppender" />-->
      <appender-ref ref="MemoryAppender"/>
    </root>

    <!--<logger name="RootLogger">
      <level value="ALL"/>
      <appender-ref ref="MemoryAppender"/>
    </logger>-->

  </log4net>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/></startup></configuration>
